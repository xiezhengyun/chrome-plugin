{
	"manifest_version": 2,
	"name": "JenlinksAuxiliary",
	"version": "1.0",
	"description": "JenlinksAuxiliary",
	"author": "xiezhengyun",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["http://*/*"],
			"js": ["content-script.js"],
			"css": ["content-css.css"]
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	]
}