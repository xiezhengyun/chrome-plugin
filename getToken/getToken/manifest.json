{
	"manifest_version": 2,
	"name": "getToken",
	"version": "1.0",
	"description": "getToken",
	"author": "xiezhengyun",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"contextMenus",
		"activeTab",
		"cookies",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content-script.js"
			]
		}
	]
}